AA,KAAA;AACA;AACA,KAfA;;AAgBA;AACA,IAAA,QAAA,GAAA;AACA,YAAA,QAAA,GAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,IAAA,EAAA,GAFA,CAEA;;AAFA,OAAA;AAIA,WAAA,MAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,EAAA,IAAA,CAAA,QAAA,IAAA;AACA,cAAA;AAAA,UAAA;AAAA,YAAA,QAAA;;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,eAAA,UAAA,GAAA,IAAA,CAAA,IAAA,CAAA,OAAA,IAAA,IAAA,CAAA,IAAA,CADA,CACA;AACA,SAFA,MAEA;AACA,eAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,KAAA,IAAA;AACA,QAAA,OAAA,CAAA,KAAA,CAAA,SAAA,EAAA,KAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA,OAVA;AAWA,KAjCA;;AAkCA;AACA,IAAA,WAAA,GAAA;AACA,WAAA,WAAA,CAAA,OAAA,GAAA,CAAA;AACA,WAAA,OAAA;AACA,KAtCA;;AAuCA;AACA,IAAA,UAAA,GAAA;AACA,WAAA,WAAA,GAAA;AACA,QAAA,OAAA,EAAA,CADA;AAEA,QAAA,QAAA,EAAA,EAFA;AAGA,QAAA,IAAA,EAAA,EAHA;AAIA,QAAA,KAAA,EAAA,SAJA;AAKA,QAAA,SAAA,EAAA,KAAA,WAAA,CAAA;AALA,OAAA;AAOA,WAAA,OAAA;AACA,KAjDA;;AAkDA;AACA,IAAA,UAAA,CAAA,GAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,mBAAA,GAAA,CAAA,EAAA,EAAA;AACA,KArDA;;AAsDA;AACA,IAAA,YAAA,CAAA,GAAA,EAAA;AACA,WAAA,QAAA,CAAA,YAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA;AACA,gBAAA,GAAA,GAAA,MAAA,cAAA,CAAA,GAAA,CAAA,EAAA,CAAA;;AACA,cAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,iBAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,iBAAA,OAAA;AACA;AACA,SANA,CAMA,OAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,SAAA,EAAA,KAAA;AACA;AACA,OAdA;AAeA,KAvEA;;AAwEA;AACA,IAAA,gBAAA,CAAA,GAAA,EAAA;AACA,WAAA,WAAA,CAAA,QAAA,GAAA,GAAA;AACA,WAAA,OAAA;AACA,KA5EA;;AA6EA;AACA,IAAA,mBAAA,CAAA,GAAA,EAAA;AACA,WAAA,WAAA,CAAA,OAAA,GAAA,GAAA;AACA,WAAA,OAAA;AACA;;AAjFA;AArBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-card class=\"box-card\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span>我的帖子管理</span>\r\n      </div>\r\n      \r\n      <!-- 搜索栏 -->\r\n      <el-form :inline=\"true\" :model=\"queryParams\" class=\"demo-form-inline\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"queryParams.name\" placeholder=\"请输入标题\" clearable />\r\n        </el-form-item>\r\n        <el-form-item label=\"分类\">\r\n          <el-select v-model=\"queryParams.tagId\" placeholder=\"请选择分类\" clearable>\r\n            <el-option\r\n              v-for=\"item in tagOptions\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item>\r\n          <el-button type=\"primary\" @click=\"handleQuery\">查询</el-button>\r\n          <el-button @click=\"resetQuery\">重置</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <!-- 帖子列表 -->\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"postList\"\r\n        style=\"width: 100%\"\r\n        border\r\n      >\r\n        <el-table-column prop=\"cover\" label=\"封面\" width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <el-image \r\n              style=\"width: 100px; height: 60px\"\r\n              :src=\"scope.row.cover\"\r\n              :preview-src-list=\"[scope.row.cover]\"\r\n              fit=\"cover\"\r\n            />\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"tagName\" label=\"分类\" width=\"120\" />\r\n        <el-table-column prop=\"name\" label=\"标题\" min-width=\"200\" />\r\n        <el-table-column prop=\"createTime\" label=\"发布时间\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            {{ scope.row.createTime | parseTime }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"viewsNumber\" label=\"浏览量\" width=\"100\" />\r\n        <el-table-column label=\"操作\" width=\"150\" fixed=\"right\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              type=\"text\"\r\n              size=\"small\"\r\n              @click=\"handleView(scope.row)\"\r\n            >\r\n              查看\r\n            </el-button>\r\n            <el-button\r\n              type=\"text\"\r\n              size=\"small\"\r\n              class=\"delete-btn\"\r\n              @click=\"handleDelete(scope.row)\"\r\n            >\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!-- 分页 -->\r\n      <div class=\"pagination-container\">\r\n        <el-pagination\r\n          background\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"queryParams.pageNum\"\r\n          :page-sizes=\"[10, 20, 30, 50]\"\r\n          :page-size=\"queryParams.pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n        />\r\n      </div>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { queryUserNews, deleteUserNews } from '@/api/news'\r\n\r\nexport default {\r\n  name: 'UserPostManage',\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      postList: [],\r\n      total: 0,\r\n      tagOptions: [],\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        tagId: undefined,\r\n        publisher: undefined\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.getList()\r\n    this.loadTags()\r\n  },\r\n  methods: {\r\n    // 获取帖子列表\r\n    async getList() {\r\n      this.loading = true\r\n      try {\r\n        const res = await queryUserNews(this.queryParams)\r\n        if (res.code === 200) {\r\n          this.postList = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      } catch (error) {\r\n        console.error('获取帖子列表失败:', error)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    // 获取分类选项\r\n    loadTags() {\r\n      const queryDto = {\r\n        current: 1,\r\n        size: 100  // 设置一个较大的值以获取所有标签\r\n      }\r\n      this.$axios.post('/tags/query', queryDto).then(response => {\r\n        const { data } = response\r\n        if (data.code === 200) {\r\n          this.tagOptions = data.data.records || data.data  // 兼容分页和非分页返回\r\n        } else {\r\n          this.$message.error('标签加载失败')\r\n        }\r\n      }).catch(error => {\r\n        console.error('加载标签失败:', error)\r\n        this.$message.error('标签加载失败')\r\n      })\r\n    },\r\n    // 查询按钮点击\r\n    handleQuery() {\r\n      this.queryParams.pageNum = 1\r\n      this.getList()\r\n    },\r\n    // 重置按钮点击\r\n    resetQuery() {\r\n      this.queryParams = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        name: '',\r\n        tagId: undefined,\r\n        publisher: this.queryParams.publisher\r\n      }\r\n      this.getList()\r\n    },\r\n    // 查看帖子\r\n    handleView(row) {\r\n      this.$router.push(`/news-detail?id=${row.id}`)\r\n    },\r\n    // 删除帖子\r\n    handleDelete(row) {\r\n      this.$confirm('确认要删除该帖子吗？', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        try {\r\n          const res = await deleteUserNews(row.id)\r\n          if (res.code === 200) {\r\n            this.$message.success('删除成功')\r\n            this.getList()\r\n          }\r\n        } catch (error) {\r\n          console.error('删除帖子失败:', error)\r\n        }\r\n      })\r\n    },\r\n    // 分页大小改变\r\n    handleSizeChange(val) {\r\n      this.queryParams.pageSize = val\r\n      this.getList()\r\n    },\r\n    // 页码改变\r\n    handleCurrentChange(val) {\r\n      this.queryParams.pageNum = val\r\n      this.getList()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.app-container {\r\n  padding: 20px;\r\n}\r\n\r\n.pagination-container {\r\n  margin-top: 20px;\r\n  text-align: right;\r\n}\r\n\r\n.delete-btn {\r\n  color: #F56C6C;\r\n}\r\n\r\n.delete-btn:hover {\r\n  color: #f78989;\r\n}\r\n</style> "],"sourceRoot":"src/views/user"}]}