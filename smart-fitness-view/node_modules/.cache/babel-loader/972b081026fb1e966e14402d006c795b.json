A,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,YAAA;AACA,gBAAA,GAAA,GAAA,MAAA,YAAA,CAAA,GAAA,CAAA,EAAA,CAAA;;AACA,cAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,iBAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,iBAAA,SAAA;AACA;AACA,SANA,CAMA,OAAA,KAAA,EAAA;AACA,eAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAdA;AAeA,KAjCA;;AAkCA,IAAA,QAAA,CAAA,IAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,KArCA;;AAsCA,IAAA,gBAAA,CAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,SAAA;AACA,KAzCA;;AA0CA,IAAA,mBAAA,CAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,SAAA;AACA,KA7CA;;AA8CA,IAAA;AA9CA,GAbA;;AA6DA,EAAA,OAAA,GAAA;AACA,SAAA,SAAA;AACA;;AA/DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"my-posts-container\">\r\n    <el-card class=\"box-card\">\r\n      <template #header>\r\n        <div class=\"card-header\">\r\n          <span>我的帖子</span>\r\n        </div>\r\n      </template>\r\n      \r\n      <el-table :data=\"posts\" style=\"width: 100%\" v-loading=\"loading\">\r\n        <el-table-column prop=\"cover\" label=\"封面\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            <el-image \r\n              style=\"width: 100px; height: 60px\"\r\n              :src=\"scope.row.cover\"\r\n              fit=\"cover\"\r\n              :preview-src-list=\"[scope.row.cover]\">\r\n            </el-image>\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column prop=\"name\" label=\"标题\" min-width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <el-link type=\"primary\" @click=\"viewPost(scope.row)\">{{ scope.row.name }}</el-link>\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column prop=\"viewCount\" label=\"浏览次数\" width=\"120\" />\r\n        \r\n        <el-table-column prop=\"createTime\" label=\"发布时间\" width=\"180\">\r\n          <template slot-scope=\"scope\">\r\n            {{ timeAgo(scope.row.createTime) }}\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column label=\"操作\" width=\"120\" fixed=\"right\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              type=\"danger\"\r\n              size=\"small\"\r\n              @click=\"handleDelete(scope.row)\"\r\n            >删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      \r\n      <div class=\"pagination-container\">\r\n        <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"currentPage\"\r\n          :page-sizes=\"[10, 20, 30, 50]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </el-card>\r\n    \r\n    <!-- 帖子详情对话框 -->\r\n    <el-dialog\r\n      :visible.sync=\"dialogVisible\"\r\n      title=\"帖子详情\"\r\n      width=\"70%\"\r\n    >\r\n      <div v-if=\"currentPost\">\r\n        <h2>{{ currentPost.name }}</h2>\r\n        <div class=\"post-info\">\r\n          <span>发布时间：{{ timeAgo(currentPost.createTime) }}</span>\r\n          <span>浏览次数：{{ currentPost.viewCount }}</span>\r\n        </div>\r\n        <div class=\"post-content\" v-html=\"currentPost.content\"></div>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getMyPosts, deleteMyPost } from '@/api/news'\r\nimport { timeAgo } from '@/utils/data'\r\n\r\nexport default {\r\n  name: 'MyPosts',\r\n  data() {\r\n    return {\r\n      posts: [],\r\n      loading: false,\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogVisible: false,\r\n      currentPost: null\r\n    }\r\n  },\r\n  methods: {\r\n    async loadPosts() {\r\n      this.loading = true\r\n      try {\r\n        const res = await getMyPosts({\r\n          current: this.currentPage,\r\n          size: this.pageSize\r\n        })\r\n        if (res.code === 200) {\r\n          this.posts = res.data.records\r\n          this.total = res.data.total\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('获取帖子列表失败')\r\n      }\r\n      this.loading = false\r\n    },\r\n    handleDelete(row) {\r\n      this.$confirm('确定要删除这篇帖子吗？', '警告', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        try {\r\n          const res = await deleteMyPost(row.id)\r\n          if (res.code === 200) {\r\n            this.$message.success('删除成功')\r\n            this.loadPosts()\r\n          }\r\n        } catch (error) {\r\n          this.$message.error('删除失败')\r\n        }\r\n      })\r\n    },\r\n    viewPost(post) {\r\n      this.currentPost = post\r\n      this.dialogVisible = true\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val\r\n      this.loadPosts()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val\r\n      this.loadPosts()\r\n    },\r\n    timeAgo\r\n  },\r\n  created() {\r\n    this.loadPosts()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.my-posts-container {\r\n  padding: 20px;\r\n}\r\n\r\n.card-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.pagination-container {\r\n  margin-top: 20px;\r\n  display: flex;\r\n  justify-content: flex-end;\r\n}\r\n\r\n.post-info {\r\n  margin: 10px 0;\r\n  color: #666;\r\n}\r\n\r\n.post-info span {\r\n  margin-right: 20px;\r\n}\r\n\r\n.post-content {\r\n  margin-top: 20px;\r\n  line-height: 1.6;\r\n}\r\n</style> "],"sourceRoot":"src/views/user"}]}