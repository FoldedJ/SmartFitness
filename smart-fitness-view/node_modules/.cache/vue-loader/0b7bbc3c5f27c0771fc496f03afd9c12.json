newsData = data.data;\r\n                this.total = data.total;\r\n            } catch (e) {\r\n                console.error(`搜索资讯列表加载异常：${e}`);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.news-tags {\r\n    display: inline-block;\r\n    padding: 2px 5px;\r\n    background-color: rgb(222, 243, 251);\r\n    color: #1d3cc4;\r\n    border-radius: 3px;\r\n}\r\n\r\n.new-item:hover {\r\n    background-color: rgb(248, 248, 248);\r\n}\r\n\r\n.new-item {\r\n    margin-bottom: 10px;\r\n    padding: 10px;\r\n    box-sizing: border-box;\r\n\r\n    img {\r\n        width: 100%;\r\n        height: 118px;\r\n        border-radius: 3px;\r\n    }\r\n}\r\n\r\n.news-title {\r\n    overflow: hidden;\r\n    /* 显示省略符号来代表被修剪的文本。 */\r\n    text-overflow: ellipsis;\r\n    /* 文本不换行 */\r\n    white-space: nowrap;\r\n}\r\n</style>"]}]}